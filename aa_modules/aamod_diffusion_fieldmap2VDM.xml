<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask mfile_alias="aamod_fieldmap2VDM" domain='diffusion_session' desc='Create VDMs in fieldmaps' modality='MRI'>
            
            <qsub>
                <timeBase>0.5</timeBase>
                <memoryBase>1</memoryBase>
            </qsub>
            
            <permanenceofoutput>1</permanenceofoutput>
            
            <inputstreams>
                <stream isrenameable='1'>diffusion_data</stream>
				<stream isrenameable='1'>diffusion_dicom_header</stream>
                <stream>fieldmap</stream>
				<stream>fieldmap_dicom_header</stream>
            </inputstreams>
            
            <outputstreams>
                <stream>fieldmap</stream>
            </outputstreams>
            
            <!-- set default to emtpy if you want to avoid using false values in case of error during retrieving them from the DICOM header-->
            <te1></te1>
            <te2></te2>
            <tert></tert>
            <epifm>0</epifm>
            <kdir>-1</kdir>
            <mask>1</mask>
            <match>1</match>
            <writeunwarpedEPI>1</writeunwarpedEPI>
            
        </currenttask>
    </tasklist>
</aap>
